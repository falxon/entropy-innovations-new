<!doctype html>
<html lang="en">
  <head profile="http://www.w3.org/2005/10/profile">
    <!-- Required meta tags -->
    <meta name="description" content="{{metadescription}}">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon"
      type="image/ico"
      href="/images/entropy-innovations-favicon.ico">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <!--<link rel="stylesheet" href="/css/bootstrap.css">-->
    <link href="modules/simple-line-icons/css/simple-line-icons.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="/css/custom.css">
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.0.3/dist/leaflet.css" crossorigin="" integrity="sha512-07I2e+7D8p6he1SIM+1twR5TIrhUQn9+I6yjqD53JQjFiMf8EtC93ty0/5vJTZGF8aAocvHYNEDJajGdNx1IsQ==">
    <script src="https://unpkg.com/leaflet@1.0.3/dist/leaflet.js" integrity="sha512-A7vV8IFfih/D732iSSKi20u/ooOfj/AGehOKq0f4vLT1Zr2Y+RX7C+w8A1gaSasGtRUZpF/NZgzSAu4/Gc41Lg==" crossorigin=""></script>
    <script src="modules/leaflet-easymap-master/js/leaflet-easymap.js"></script>
    <script type="application/ld+json" src="/jsonld/localbusiness.jsonld"></script>
    <script type="application/ld+json" src="/jsonld/logo.jsonld"></script>

    <title>{{title}}</title>
  </head>
  <body>
<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container logo-to-left">
        <a class="navbar-brand" href="/"><img src="/images/entropy-innovations-logo-less-smol.png" alt="Entropy Innovations" title="Entropy Innovations"></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav ml-auto">
            {{#nav_item}}
            <li class="nav-item {{active}}">
              <a class="nav-link" href="{{url}}">{{nav}} {{#sr_active}}<span class="sr-only">{{(current)}}</span>{{/sr_active}}</a>
            </li>
            {{/nav_item}}
            {{#nav_drop}}
            <li class="nav-item dropdown">
              <a class="nav-link dropdown-toggle" href="{{url}}" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                {{nav}}{{#sr_active}}<span class="sr-only">{{(current)}}</span>{{/sr_active}}
              </a>
              <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                {{#drop_item}}
                <a class="dropdown-item" href="{{url}}">{{item}}</a>
                {{/drop_item}}
              </div>
            </li>
            {{/nav_drop}}
          </ul>
        </div>
        </div>
      </nav>

      {{#header}}
      <header class="text-white text-center {{class}}">
        <div class="container">
          <div class="row">
            <div class="col-sm-8 mx-auto h1-big">
              <h1 class="h1-big">{{text}}</h1>
            </div>
          </div>
        </div>
      </header>
      {{/header}}

      {{#main}}
      <div class="container bg-primary">
        {{#alert}}
        <div class="alert alert-{{type}}" role="primary">
          {{message}}
        </div>
        {{/alert}}
        <div class="row">
          <div class="col-sm-12 px-lg-6 px-sm-5 px-4 links-normal">
            {{#title}}<h2 class="mt-4 mb-2 text-center">{{text}}</h2>{{/title}}
            {{#section}}
            {{#icons}}
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12">

            <div class="features-icons-item mx-auto mb-5 mb-lg-4 mt-4">
              <div class="features-icons-icon d-flex">
                {{#icon}}
                <i class="{{icon_class}} m-auto text-white text-large"></i>
                {{/icon}}
              </div>
            </div>
        </div>
        </div>
        <div class="row">
          <div class="col-sm-12">
            {{/icons}}
            {{#divide}}
          </div>
        </div>
      </div>
      <div class="{{class}}">
      </div>
        <div class="container bg-primary">
          <div class="row">
            <div class="col-sm-12 px-lg-6 px-sm-5 px-4 links-normal">
            {{/divide}}
            {{#subtitle}}<h{{num}} class="{{class}} {{class2}}">{{subtext}}</h{{num}}>{{/subtitle}}
            {{#paragraph}}<p>{{{text}}}</p>{{/paragraph}}
            {{#buttons}}
          </div>
        </div>
        <div class="row">
          <div class="col-sm-12 text-center">
            {{#button}}
            <a class="btn btn-light mx-2 mx-sm-3 mx-md-4 mx-lg-5 mb-4" href="/{{url}}">{{text}}</a>
            {{/button}}
          </div>
        </div>
        <div class="row">
          <div class="col-sm-12">

            {{/buttons}}
            {{/section}}
            {{#images}}
            <div class="row">
              <div class="text-center col-sm-{{cols}} {{class}}">
                {{#image}}<img src="{{url}}" alt="{{alt}}">{{/image}}
              </div>
            </div>
            {{/images}}
            <div class="text-center">
              <a href="https://twitter.com/entropy_innova?ref_src=twsrc%5Etfw" class="twitter-follow-button btn btn-light mb-4" data-show-count="false">Follow @entropy_innova</a>
              <script async type="text/javascript" src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>

            </div>
          </div>
        </div>
      </div>
      {{/main}}

      {{#portfolio}}
      <div class="container bg-primary">
        <div class="row">
          <div class="col-sm-12 px-lg-6 px-sm-5 px-4 links-normal">
            <h2 class="text-center mb-4 mt-4">{{title}}</h2>
            <p>{{paragraph}}</p>
            {{#site}}
            {{#image}}
            <div class="row">
              <div class="col-sm-12 text-center">
                <a href="{{url}}"><img class="img-fluid img-thumbnail mb-3" src="{{src}}" alt="{{alt}}"></a>
              </div>
            </div>
            {{/image}}
            {{#text}}
            <div class="row">
              <div class="col-sm-12">
                <p>{{content}}</p>
                <a href="{{url}}" class="btn btn-dark float-right mb-3">{{button}}</a>
              </div>
            {{/text}}
            </div>
            {{/site}}
          </div>
        </div>
      </div>
      {{/portfolio}}

      {{#form}}
      <div class="container bg-primary">
        <div class="row">
          <div class="col-sm-12 px-lg-6 px-sm-5 px-4 links-normal">
            <h2 class="text-center mb-4 mt-4">{{title}}</h2>
            <div class="card card-body bg-dark mb-4">
            <form method="post" action="/home">
              <div class="form-group">
                <label for="name">Name</label>
                <input type="text" class="form-control" name="name" required="true" placeholder="Enter name here">
              </div>
              <div class="form-group">
                <label for="email">Email address</label>
                <input type="email" required="true" class="form-control" name="email" placeholder="Enter email here">
              </div>
              <div class="form-group">
                <label for="subject">Subject</label>
                <input type="text" required="true" class="form-control" name="subject" placeholder="Subject">
              </div>
              <div class="form-group">
                <label for="topic">Topic</label>
                <select class="form-control" id="topic" name="topic">
                  {{#option}}<option value="{{value}}" {{selected}}>{{name}}</option>{{/option}}
                </select>
              </div>
              <div class="form-group" id="project">
                <label for="project">Project</label>
                <input type="text" class="form-control" name="project" placeholder="Please tell us where we know you from">
              </div>
              <div class="form-group">
                <label for="message">Message</label>
                <textarea name="message" required="true" class="form-control" rows="5" placeholder="Please tell us how we can help you"></textarea>
              </div>
              <div class="card-footer">
                <button type="submit" class="btn btn-light float-right">Submit</button>
              </div>
            </form>
          </div>
            {{#contact}}
            <div class="row mb-4">
            {{#map}}
              <!---<div class="col-sm-6">
                <div class="leasymap" data-origin="{{coordinate}}" data-marker="51.512829, -0.087231" data-zoom="12">
                </div>
              </div>--->
              <div class="col-sm-6"><iframe class="mapa" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2483.021095144209!2d-0.08942504863285737!3d51.51282897953604!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x0!2zNTHCsDMwJzQ2LjIiTiAwwrAwNScxNC4wIlc!5e0!3m2!1sen!2suk!4v1522577937874" frameborder="0" style="border:0" allowfullscreen></iframe></div>
            {{/map}}
            <div class="col-sm-6">
              <p><strong>Find us at:</strong></p>
              <p>Entropy Innovations<br/>
              68 Lombard Street<br/>
              London<br/>
              EC3V 9LJ</p>
              <p><strong>Meetings by Appointment Only</strong></p>
              <p><strong>Or call us on: {{phone_number}}</strong>
            </div>
            </div>
            {{/contact}}
          </div>
        </div>
      </div>
      {{/form}}


      <div class="container bottom-fractal">
        <div class="row">
          <div class="col-sm-12">
          </div>
        </div>
      </div>

      <footer class="footer bg-dark">
        <div class="container text-white links-white">
          <div class="row">
            <div class="col-sm-4 my-auto py-lg-3 px-lg-1 my-auto">
              <ul>
                {{#nav_item}}
                <li><a href="{{url}}">{{nav}}</a></li>
                {{/nav_item}}
                {{#nav_drop}}
                <li><a href="{{url}}">{{nav}}</a></li>
                {{/nav_drop}}
              </ul>
            </div>
            <div class="col-sm-4 text-center my-auto">
              <p class="mb-lg-0">&copy; {{site_name}}</p>
            </div>
            <div class="col-sm-4 text-right my-auto py-lg-3 px-lg-3">
              <p>Call us on {{phone}}<br/>
              Or send a message<br/> using the <a href="/contact" class="underline">Contact Form</a></p>
            </div>
          </div>
        </div>
      </footer>

    </div>
  </div>
</div >


    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script type="text/javascript" src="/javascript/custom.js"></script>
  </body>
</html>
